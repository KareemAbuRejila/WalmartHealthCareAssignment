    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.dotech.walmarthealthcareassignment.app.ui.countries.CountriesFragment"
        android:label="@string/country_list"
        tools:layout="@layout/fragment_countries" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.dotech.walmarthealthcareassignment.app.ui.settings.SettingFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.dotech.walmarthealthcareassignment.app.ui.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile" />
